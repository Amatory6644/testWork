<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyTheme" parent="Theme.Material3.Light.NoActionBar">

        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="CustomButtonMaterial" parent="Widget.MaterialComponents.Button">
        <item name="materialThemeOverlay">@style/ButtonThemeOverlay</item>
    </style>
    <style name="ButtonThemeOverlay">
        <item name="android:minHeight">0dp</item>
        <item name="colorPrimary">@color/Dark_Fon</item>
        <item name="strokeColor">@color/black</item>
        <item name="strokeWidth">1dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="boxCornerRadiusTopEnd">28px</item>
        <item name="boxCornerRadiusBottomEnd">28px</item>
        <item name="boxCornerRadiusBottomStart">28px</item>
        <item name="boxCornerRadiusTopStart">28px</item>
    </style>
</resources>